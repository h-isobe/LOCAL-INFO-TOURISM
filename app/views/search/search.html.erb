<div class="container"> 
  <div class="row">

  <% if @model == 'user' %>
    <h3 class="text-center">Users search for '<%= @content %>' </h3>
    <%= render 'users/index' , users: @users %>
  <% elsif @model == 'post' %>
    <h3 class="text-center">Posts search for '<%= @content %>' </h3>
    <%= render 'posts/index' , posts: @posts %>
  <% end %> 

  </div>
</div>